logging:
  log_file: log.txt
  log_level: 3        # 0: no log, 1: error, 2: warning, 3: info, 4: debug
  print_log: true     # print log to the console?

data:
  conversations_path: data/conversations
  assistants_path: data/assistants
  agents_path: data/agents
  users_path: data/users
  user_prompt_templates_path: data/prompt_templates/user_prompt_templates
  system_prompt_templates_path: data/prompt_templates/system_prompt_templates

llm:
  provider: "openai"
  model: "gpt-4o-mini"
  api_key: <set-your-openai-api-key-here>

connections:
  products_db:
    host: localhost
    database: products
    user: root
    password: <set-your-password-here>